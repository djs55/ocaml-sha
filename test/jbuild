(jbuild_version 1)

(executable
 ((name shasum)
  (modules (shasum))
  (libraries (sha))))

(executable
 ((name shatest)
  (modules (shatest))
  (libraries (sha oUnit))))

(alias
 ((name runtest)
  (deps (shatest.exe sample.txt))
  (action (run ${<}))))

(executable
 ((name bench)
  (modules (bench))
  (libraries (sha benchmark))))
